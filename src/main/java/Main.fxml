<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx" prefHeight="400.0" prefWidth="600.0">
<padding><Insets top="1" right="1" bottom="1" left="1"/></padding>
<children>
    <Button fx:id="menu" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0"/>

    <GridPane fx:id="navList" style="-fx-background-color:white"
              AnchorPane.topAnchor="0.0"
              AnchorPane.bottomAnchor="0.0"
              prefWidth="100.0"
              translateX="-100"
              vgap="4"
              alignment="TOP_CENTER">
        <padding><Insets top="25" right="25" bottom="25" left="25"/></padding>
        <children>
            <Button fx:id="storeBtn" text="store" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <Button fx:id="profileBtn" text="Profile" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <Button fx:id="settingsBtn" text="Settings" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <Button text="item1" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
            <Button fx:id="logoutBtn" text="Logout" GridPane.rowIndex="4" GridPane.columnIndex="0"/>
        </children>
    </GridPane>
</children>
</fx:root>
